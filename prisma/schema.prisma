generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Delivery {
  id         String   @id @default(cuid())
  address   String
  total    Int
  type    String
  businessName String
  businessId String
  jan_total Int
  feb_total Int
  mar_total Int
  apr_total Int
  may_total Int
  jun_total Int
  jul_total Int
  aug_total Int
  sep_total Int
  oct_total Int
  nov_total Int
  dec_total Int
}